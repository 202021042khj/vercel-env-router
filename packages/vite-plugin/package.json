{
  "name": "@vercel-env-router/vite",
  "version": "0.1.0",
  "description": "Vite plugin for automatic vercel.json generation",
  "keywords": ["vite", "plugin", "vercel", "environment"],
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/202021042khj/vercel-env-router.git",
    "directory": "packages/vite-plugin"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@vercel-env-router/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0"
  }
}
